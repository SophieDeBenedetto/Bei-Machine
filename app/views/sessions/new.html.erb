<h1>Log In to use the Bei Machine</h1>
<p id="notice"><%= notice %></p>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <%= submit_tag "Log In" %>
<% end %>